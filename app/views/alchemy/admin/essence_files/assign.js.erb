$('#<%= @content.dom_id %>').replaceWith('<%= j(
  render "alchemy/essences/essence_file_editor",
    formats: [:html],
    content: @content,
    options: @options
) %>');
Alchemy.closeCurrentWindow();
Alchemy.setElementDirty($('#element_<%= @content.element.id %>'));
Alchemy.overlayObserver($('#<%= @content.dom_id %>'));
