<div class="add_picture" id="add_picture_<%= element.id %>">
  <%= link_to_overlay_window(
    render_icon('add'),
    alchemy.admin_pictures_path(
      :element_id => element,
      :options => options
    ),
    {
      :title => t('add_image_to_element'),
      :size => '780x580',
      :resizable => true
    }
  ) %>
</div>
