(function($) {
	$('#locked_page_<%= @page_id -%>').remove();
	<%- if @layoutpage -%>
	window.location.replace('<%= layoutpages_admin_pages_path -%>')
	<%- elsif @page_root -%>
	$('#sitemap').replaceWith('<%= escape_javascript(render(:partial => "sitemap")) -%>');
	Alchemy.growl('<%= @message -%>');
	Alchemy.Tooltips();
	<%- else -%>
	window.location.replace('<%= admin_pages_path -%>');
	<%- end -%>
})(jQuery);
