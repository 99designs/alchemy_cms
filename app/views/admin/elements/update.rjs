page.replace(
  "element_#{@element.id}",
  :partial => "element",
  :object => @element
)
page << "jQuery('#element_area').sortable('refresh')"
Alchemy::Notice.show(page, _("element_saved"))
page << "Alchemy.PreviewWindow.refresh()"
page << "Alchemy.ElementEditorSelector()"
@richtext_contents.each do |content|
  page << "TinymceHammer.addEditor('contents_content_#{content.id}')"
end
