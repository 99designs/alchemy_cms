page.remove("element_#{@element.id}")
if @page.elements.empty?
  page << remote_function(:url => {:controller => "elements", :action => "new", :page_id => @page.id, :open => true})
else
  page << "jQuery('#element_area').sortable('refresh')"
end
Alchemy::Notice.show(page, _("element_deleted_successful"))
page << "Alchemy.PreviewWindow.refresh()"
