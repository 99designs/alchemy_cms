<div class="bild_mit_text" id="<%= element_dom_id(element) %>">
	<div class="bild_mit_text_images">
		<%- element.contents.find_all_by_essence_type("EssencePicture").each do |atom| -%>
		<div class="bild_mit_text_image">
			<%= atom.atom.crop_from %>
			<%= atom.atom.crop_size %>
			<%= render_atom_view(atom, :image_size => "250x200", :crop => true) %>
		</div>
		<%- end -%>
	</div>
	<div class="headline">
    	<%= render_atom_view_by_name(element, 'headline') %>
	</div>
	<div class="text">
		<%= render_atom_view_by_name(element, 'text') %>
	</div>
</div>
