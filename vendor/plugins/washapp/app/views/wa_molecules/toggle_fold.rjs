if @error
  page.replace("molecule_#{@wa_molecule.id}_folder_spinner", "<span class='error_icon' title='#{@error}'>!</span>")
else
  page.replace(
    "dragable_#{@wa_molecule.id}",
    :partial => "wa_molecule",
    :object => @wa_molecule,
    :locals => {:open_molecule_selector => false}
  )
  WaConfigure.sortable_molecules(page)
  page << "setFrameSize()"
  page << "TinymceHammer.init()"
end