<li>
	<div class="sitemap_left_images">
		<%= render(
			:partial => 'wa_page_status',
			:locals => {
				:wa_page => systempage
			}
		) -%>
	</div>
	<div class="wa_sitemap_sitename">
	<% unless systempage.locked_for(current_user) -%>
	<%= link_to(
			systempage.name, {
				:controller => 'wa_pages',
				:action => 'edit_content',
				:id => systempage,
				:systempage => true
			},
			:title => _("edit_page"),
			:class => "wa_sitemap_pagename_link"
		) -%>
	<% else -%>
		<%= systempage.name %>
	<% end -%>
	</div>
</li>
