if @after_molecule.nil?
  page.insert_html(
    :top,
    "molecule_area",
    :partial => "molecule",
    :object => @new_molecule
  )
else
  page.insert_html(
    :after,
    "dragable_#{@after_molecule.id}",
    :partial => "molecule",
    :object => @new_molecule
  )  
end
Alchemy::Configuration.sortable_molecules(page)
Alchemy::Notice.show_via_ajax(page, _("successfully_added_element"))
page << "setFrameSize()"
page << "TinymceHammer.init()"
