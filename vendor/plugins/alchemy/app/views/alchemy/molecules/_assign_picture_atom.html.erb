<div class="wa_add_atom wa_assign_atom" id="assign_atom_<%= atom.molecule.id %>_<%= atom.id %>">
	<%= link_to_wa_window(
		"",
		url_for(
			:controller => 'wa_images',
			:action => 'archive_overlay',
			:molecule_id => atom.molecule,
			:atom_id => atom,
			:options => options
		),
		{
			:title => _('assign_image'),
			:size => '780x450',
			:resizable => 'true'
		},
		:class => 'assign',
		:title => _('assign_image')
	) %>
</div>
